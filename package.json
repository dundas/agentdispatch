{
  "name": "agent-dispatch",
  "version": "1.0.0",
  "description": "Agent Dispatch Messaging Protocol (ADMP) - A Universal Inbox Standard for Autonomous Agents",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces",
    "dev": "npm run dev -w @agent-dispatch/core",
    "docker:up": "docker-compose -f deploy/docker-compose.yml up -d",
    "docker:down": "docker-compose -f deploy/docker-compose.yml down",
    "docker:logs": "docker-compose -f deploy/docker-compose.yml logs -f"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agent-dispatch/agent-dispatch"
  },
  "keywords": [
    "agents",
    "messaging",
    "protocol",
    "inbox",
    "admp",
    "autonomous-agents",
    "ai-agents"
  ],
  "author": "Agent Dispatch Working Group",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "agentbootup": "file:../agentbootup"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "prettier": "^3.1.0",
    "eslint": "^8.56.0"
  }
}
